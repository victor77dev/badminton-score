{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "#/components/*": ["./src/components/*"],
      "#/hooks/*": ["./src/hooks/*"],
      "#/constants/*": ["./src/constants/*"],
      "#/assets/*": ["./src/assets/*"],
      "#/*": ["./src/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
